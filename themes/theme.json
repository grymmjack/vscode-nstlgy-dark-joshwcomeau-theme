    "editorCursor.foreground": "#d2a6fe",
    "editor.selectionBackground": "#d2a6fe3d",
    "editorIndentGuide.background": "#3e4452",
    "editorIndentGuide.activeBackground": "#3e4452",
    "editor.lineHighlightBackground": "#2c313c",
    "editor.lineHighlightBorder": "#00000000",
    "titleBar.activeBackground": "#1C1E26",
    "titleBar.inactiveBackground": "#1C1E26",
    "sideBar.background": "#1C1E26",
    "panel.background": "#1C1E26",
    "statusBar.background": "#1C1E26",
    "editorGroupHeader.tabsBackground": "#1C1E26",
    "tab.activeBackground": "#1C1E26",
    "tab.inactiveBackground": "#2F343E",
    "scrollbarSlider.background": "#4e5666",
    "scrollbarSlider.hoverBackground": "#5a6375",
    "scrollbarSlider.border": "#4e5666",
    "terminal.background": "#1C1E26",
    "terminal.ansiBlack": "#3f4451",
    "terminal.ansiRed": "#ffd500",
    "terminal.ansiGreen": "#8cc265",
    "terminal.ansiYellow": "#d18f52",
    "terminal.ansiBlue": "#d2a6ff",
    "terminal.ansiMagenta": "#c162de",
    "terminal.ansiCyan": "#42b3c2",
    "terminal.ansiWhite": "#d7dae0",
    "terminal.ansiBrightBlack": "#4f5666",
    "terminal.ansiBrightRed": "#ff616e",
    "terminal.ansiBrightGreen": "#a5e075",
    "terminal.ansiBrightYellow": "#f0a45d",
    "terminal.ansiBrightBlue": "#ff38a9",
    "terminal.ansiBrightMagenta": "#de73ff",
    "terminal.ansiBrightCyan": "#4cd1e0",
    "terminal.ansiBrightWhite": "#e6e6e6"
